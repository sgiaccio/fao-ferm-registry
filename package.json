{
  "name": "drip",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tailwind-css": "tailwind build -i src/drip/style.css -o public/css/style.css",
    "__purge-css__": "purgecss -c purgecss.config.js -o public/css",
    "__clean-css__": "cleancss -o public/css/style.css public/css/style.css",
    "release-css": "yarn tailwind-css",
    "sc": "shadow-cljs",
    "server": "shadow-cljs stop && shadow-cljs start",
    "start": "yarn tailwind-css & shadow-cljs watch app",
    "cards": "shadow-cljs watch cards",
    "build": "yarn clean && shadow-cljs release app",
    "test": "shadow-cljs compile test && node out/test.js",
    "test:watch": "shadow-cljs watch test --config-merge \"{:autorun true}\"",
    "e2e": "shadow-cljs compile e2e && node out/e2e.js",
    "report": "shadow-cljs run shadow.cljs.build-report app report.html",
    "lint": "clj-kondo --lint src",
    "format": "zprint-clj --hang -i \"./src/**/*.{clj,cljs,cljc,edn}\" -o ./ && zprint-clj -i \"./*.edn\" -o ./",
    "clean": "rimraf public/js"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.19.0",
    "@testing-library/react": "^10.4.3",
    "autoprefixer": "^10.2.6",
    "clj-kondo": "2020.11.7",
    "highlight.js": "10.1.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "marked": "^1.1.0",
    "postcss": "^8.3.5",
    "react-openlayers": "^0.3.0",
    "rimraf": "~3.0.2",
    "serve-handler": "~6.1.3",
    "shadow-cljs": "~2.14.5",
    "tailwindcss": "^2.2.2",
    "zprint-clj": "~0.8.0"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "create-react-class": "~15.7.0",
    "firebase": "^9.0.0-beta.2",
    "ol": "^6.6.1",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
