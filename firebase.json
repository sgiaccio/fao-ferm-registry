{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/loadAreaJson",
        "function": "load_area_json",
        "region": "europe-west3"
      },
      {
        "source": "/loadShapefile",
        "function": "load_shapefile",
        "region": "europe-west3"
      },
      {
        "source": "/loadKmlKmz",
        "function": "load_kml_kmz",
        "region": "europe-west3"
      },
      {
        "source": "/uploadProjectFile",
        "function": "upload_project_file",
        "region": "europe-west3"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5002
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "functions": {
      "port": 5001
    },
    "pubsub": {
      "port": 8085
    },
    "singleProjectMode": true
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": {
    "source": "functions"
  },
  "extensions": {
    "firestore-send-email": "firebase/firestore-send-email@0.1.25"
  }
}
